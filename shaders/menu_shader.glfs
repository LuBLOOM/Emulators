#version 120

uniform float eTime;
uniform vec2  eResolution;

vec2 random(vec2 p)
{
	vec3 a = fract(p.xyx*vec3(123.45, 234.45, 345.45)*eTime);
	a += dot(a, a+34.45);
	return fract(vec2(a.x*a.y, a.y*a.z));
}

void main()
{
	//replace vec2(1200, 800) with eResolution when fixed
	vec2 uv = gl_FragCoord.xy/vec2(1200, 800);
	vec2 d = random(vec2(uv.y, uv.y));
	gl_FragColor = vec4(d.x, d.x, d.x, .1);
}


